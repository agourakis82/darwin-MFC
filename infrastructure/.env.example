# DARWIN-MFC INFRASTRUCTURE ENVIRONMENT
# ======================================
#
# Copy this file to .env and fill in the values.
# NEVER commit .env to version control!

# =============================================================================
# DOMAIN CONFIGURATION
# =============================================================================

# Your domain (e.g., api.yourdomain.com)
DOMAIN=mfc.agourakis.med.br

# Let's Encrypt email for SSL certificates
ACME_EMAIL=admin@agourakis.med.br

# =============================================================================
# TRAEFIK
# =============================================================================

# Dashboard auth (generate with: htpasswd -nb admin yourpassword)
TRAEFIK_DASHBOARD_AUTH=admin:$$apr1$$...

# =============================================================================
# KEYCLOAK
# =============================================================================

KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=

# =============================================================================
# POSTGRESQL
# =============================================================================

POSTGRES_USER=darwin_admin
POSTGRES_PASSWORD=

# =============================================================================
# POCKETBASE
# =============================================================================

# Generate with: openssl rand -hex 32
PB_ENCRYPTION_KEY=

# =============================================================================
# MEILISEARCH
# =============================================================================

# Generate with: openssl rand -hex 32
MEILI_MASTER_KEY=

# =============================================================================
# REDIS
# =============================================================================

# Generate with: openssl rand -hex 32
REDIS_PASSWORD=

# =============================================================================
# MINIO (Object Storage)
# =============================================================================

MINIO_ROOT_USER=darwin_admin
MINIO_ROOT_PASSWORD=

# =============================================================================
# BACKUP (Restic)
# =============================================================================

# S3-compatible backup destination
BACKUP_REPOSITORY=s3:s3.amazonaws.com/darwin-mfc-backups
BACKUP_PASSWORD=
BACKUP_S3_KEY=
BACKUP_S3_SECRET=
